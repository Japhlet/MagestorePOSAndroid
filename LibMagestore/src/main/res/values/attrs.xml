<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="magestore_view">
        <attr name="layout_span_count" format="integer" />
        <attr name="layout_item" format="reference" />
        <attr name="layout_item_progress" format="reference" />
        <attr name="layout_item_content" format="reference" />
        <attr name="layout_item_msg" format="reference" />
        <attr name="layout_list" format="reference" />
        <attr name="layout_progress" format="reference" />
        <attr name="layout_msg" format="reference" />
        <attr name="layout_panel" format="reference" />
        <attr name="layout_row" format="reference" />
        <attr name="layout_header" format="reference" />
        <attr name="layout_search" format="reference" />
        <attr name="layout_close" format="reference" />
        <attr name="layout_footer" format="reference" />
        <attr name="layout_no_scroll" format="boolean" />
        <attr name="layout_auto_height" format="boolean" />
        <attr name="page_size_max" format="integer" />
        <attr name="page_size" format="integer" />
        <attr name="page_first" format="integer" />
        <attr name="layout_orientation" format="integer" >
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="data_type" format="integer">
            <enum name="text" value="0" />
            <enum name="currency" value="1" />
            <enum name="quantity" value="2" />
            <enum name="time" value="3" />
            <enum name="date" value="4" />
        </attr>
    </declare-styleable>
</resources>