<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="magestore_view">
        <attr name="layout_cell" format="reference" />
        <attr name="layout_row" format="reference" />
        <attr name="layout_header" format="reference" />
        <attr name="layout_footer" format="reference" />
        <attr name="layout_no_scroll" format="boolean" />
        <attr name="layout_auto_height" format="boolean" />
        <attr name="prefix" format="string"/>
        <attr name="data_type" format="integer">
            <enum name="text" value="0" />
            <enum name="currency" value="1" />
            <enum name="quantity" value="2" />
            <enum name="time" value="3" />
            <enum name="date" value="4" />
        </attr>
    </declare-styleable>
</resources>